    --What are the MAX, MIN, AVG Draft Picks?
    SELECT MAX(id) AS Max_draft_picks, MIN(id) AS Min_draft_picks, AVG(id) AS Avg_draft_picks 
    FROM picks;
    
    --How many draft picks are in each skilled position? 
    SELECT position, COUNT(*) AS "Total" 
    FROM picks 
    WHERE position IN ("QB","DE", "WR", "OT", "WR", "OLB","RB","CB","DT","TE", "ILB", "OG","C","S") 
    GROUP BY position 
    ORDER BY Total DESC;
    
    --What offensive skill position went in the TOP 10?
    SELECT position, COUNT(*) AS "Top_10" 
    FROM picks 
    WHERE position IN ("QB","WR", "OT", "WR", "RB","TE", "OG","C")
    AND id <= 10 
    GROUP BY position 
    ORDER BY Top_10 DESC;
   
   --What players where drafted from Florida State?
   SELECT id,name, position, college 
   FROM picks 
   WHERE position IN ("QB","DE", "WR", "OT", "WR", "OLB","RB","CB","DT","TE", "ILB", "OG","C","S") 
   AND college LIKE "Florida State" 
   GROUP BY position 
   ORDER BY id ASC;
   
   --Compare two NFL teams top picks such as Tampa Bay and Cincinatti in the top 60. 
   SELECT pick_number, name, college, picked_by, position 
   FROM picks 
   WHERE picked_by LIKE "Tampa Bay" 
   OR picked_by LIKE "%cinc%" 
   AND pick_number <=60;
   
   --Find out what the top 20 NFL draft order.  
   SELECT Pick_number, Picked_by, COUNT(*) AS Top_20_Picks 
   FROM picks 
   GROUP BY id 
   HAVING Pick_number <=20;
   
   --San Diego's total draft picks?
   SELECT Picked_by, COUNT(*) AS Total_SanDiego_Picks 
   FROM picks 
   WHERE picked_by In ('San Diego') 
   GROUP BY 'Total_SanDiego_Picks';
   
   --Create graph showing which draft pick went in which round. 
   SELECT pick_number,	name,	college,	picked_by,	position,
   CASE
   WHEN Pick_number <= 32 THEN 'First Round Draft Pick'
   WHEN Pick_number <= 64 THEN 'Second Round Draft Pick'
   WHEN Pick_number <= 96 THEN 'Third Round Draft Pick'
   WHEN Pick_number <= 128 THEN 'Fourth Round Draft Pick'
   ELSE 'Final Round Draft Pick'
   END as 'Draft Pick'
   FROM picks; 
   
  --Break down how many players went in each draft round.  
  SELECT COUNT(*) AS Total,
   CASE
   WHEN Pick_number <= 32 then 'First Round Draft Pick'
   WHEN Pick_number <= 64 then 'Second Round Draft Pick'
   WHEN Pick_number <= 96 then 'Third Round Draft Pick'
   WHEN Pick_number <= 128 then 'Fourth Round Draft Pick'
   ELSE 'Final Round Draft Pick'
   END as "Draft Round"
   FROM picks 
   GROUP BY "Draft Round" 
   ORDER BY Total DESC; 
   
   --What CB went in the top 5? 
    SELECT id, name, position 
    FROM picks 
    GROUP BY name 
    HAVING position = "CB" 
    ORDER BY id ASC 
    LIMIT 5;
